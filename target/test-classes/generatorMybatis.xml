<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- 数据库驱动 -->
	<!-- 因为已经在有驱动的环境下，可忽略该配置 -->
	<classPathEntry
		location="D:\microjar\mysql-connector-java-6.0.6.jar" />
	<context id="ORACLETables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!--数据库链接URL，用户名、密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://132.102.104.132:3306/boke_yydz_test" userId="root"
			password="Navicat2014">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- 生成模型的包名和位置 -->
		<javaModelGenerator targetPackage="com.chinatelecom.model"
			targetProject="start-project/src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- 生成映射文件的包名和位置 -->
		<sqlMapGenerator targetPackage="com.chinatelecom.dao"
			targetProject="start-project/src/main/java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- 生成DAO的包名和位置 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.chinatelecom.dao" targetProject="start-project/src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- 要生成的表 tableName是数据库中的表名或视图名 domainObjectName是实体类名 -->
        <table schema="root" tableName="upfile" domainObjectName="UpFile"   >
			<!-- 如果设置为true，MBG不会讲schema或catalog添加到生成SQL中的表名上。 -->
	  		<property name="ignoreQualifiersAtRuntime" value="true"/>
        </table>

	</context>
</generatorConfiguration>  